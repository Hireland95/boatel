<div class="container">
  <h1>Edit your boat</h1>
  <%= simple_form_for(@boat) do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :price %>
  <%= f.input :details %>
  <%= f.input :boat_type, :collection => @boat_types %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit "Save changes" %>
<% end %>
</div>
